// T.C. = 0(n)      S.C. = O(1)
public class Solution {
    
    public static int[] countFrequency(int n, int x, int[] nums) {
        int ans[] = new int[n];
        for (int i=0; i<n; i++) {
            if (nums[i] <= n) 
                ans[nums[i] - 1]++;
    
        }
        return ans;
    }
    
}
