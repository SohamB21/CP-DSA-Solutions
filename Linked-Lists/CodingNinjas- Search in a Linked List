// T.C. = O(length of LL)       S.C. = O(1)
public class Solution
{
    public static int searchInLinkedList(Node head, int k)
    {
        Node temp = head;
        while(temp != null){
            if(temp.data == k)
                return 1;
            temp = temp.next;
        }
        return 0;
    }
}
